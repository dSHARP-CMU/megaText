   "source": [
    "result = result.reset_index()\n",
    "result = result.rename(columns={3:\"genres\"})\n",
    "\n",
    "mylists = [list(x) for x in list(result['genres'])]\n",
    "\n",
    "#crude version of looping through the list of lists to remvoe \"none\" from each row\n",
    "q = []\n",
    "for i in mylists:\n",
    "    p = []\n",
    "    #loop through each item per row \n",
    "    for d in i:\n",
    "        #if the item isn't \"none\", keep it\n",
    "        if d != 'none':\n",
    "            p.append(d)\n",
    "    #append the fixed row to a list of rows\n",
    "    if len(p) > 0:\n",
    "        q.append(\", \".join(p))\n",
    "    else:\n",
    "        q.append(\"none\")\n",
    "#set the new list of rows (q) to the dataframe column\n",
    "result[\"genres\"] = q\n",
    "result"
   ]
